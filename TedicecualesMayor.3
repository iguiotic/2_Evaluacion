#include<stdio.h>

int devuelvemayor(int num1,int num2,int num3){
	int respuesta;
	if(num1>num2 && num1>num3) respuesta=num1;
	if(num2>num1 && num2>num3) respuesta=num2;
	if(num3>num2 && num3>num1) respuesta=num3;
	
	return respuesta;
}
int main(){
	int num1,num2,num3;
	printf("introduce 3 numeros enteros:\n");
	
	printf("numero 1:");
	scanf("%d",&num1);
	
	printf("numero 2:");
	scanf("%d",&num2);
	
	printf("numero 3:");
	scanf("%d",&num3);
	
	printf("el mayor de %d, %d y %d es: ",num1,num2,num3);
	printf("%d",devuelvemayor(num1,num2,num3));	
	
	
	
	
	
	
	
	return 0;
}
